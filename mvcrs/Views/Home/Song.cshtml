@{
    ViewData["Title"] = "Song";
}
<style>
    .main {
        background-color: rgb(169, 169, 169);
        display: flex;
        flex-direction: column;
    }
</style>

<!DOCTYPE html>
<html lang="en">

<head>
    <title>Miracle Song Lyrics</title>
    <script type="text/javascript"> ArtistName = "Sia"; SongName = "Miracle";</script>
    <script
        src="https://code.jquery.com/jquery-3.5.1.min.js"
        integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"></script>
    @* <script src="https://unpkg.com/axios/dist/axios.min.js"></script> *@
    <script>
        $(function () {
            $('#btn-all').on('click', function () {
                $.get(
                    'http://localhost:5000/Song/Miracle',
                    function (response) {
                        console.log("> ", response);
                        $("#result").html(response);
                });
            });
        });
    </script>
</head>

<body class="main">
    <!-- content -->
    <div class="lyricsh">
        <h2><b>Miracle Lyrics</b></h2>
    </div>
    <img src="~/images/Miracle.jfif"
        alt="" class="image-style">
    <br />
    <br />
    <button id="btn-all">Get Lyrics</button>
    <br />
    <div id="result"></div>
</body>

</html>